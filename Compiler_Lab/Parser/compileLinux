while [ $# -gt 0 ]
do
    case "$1" in
    	-o)  echo "Test -o 2: "$2; shift ;;
        clean)  echo "clean -v : "$1;;
        compile)  echo "compile -b : "$1;;
        run)  echo "Test -run 1: "$1;
		echo "Test -run 2: "$2; 
		echo "Test -run 3: "$3; ;;
    esac
    shift
done

